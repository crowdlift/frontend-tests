{
  "name": "frontendkit-tests",
  "version": "0.2.4",
  "description": "FrontendKit test harness.",
  "private": true,
  "main": "./index.js",
  "bin": {
    "karma": "./node_modules/.bin/karma",
    "mocha": "./node_modules/.bin/mocha",
    "nightwatch": "./node_modules/.bin/nightwatch",
    "test_e2e_local": "./bin/test_e2e_local.sh"
  },
  "repository": "crowdlift/frontendkit-tests",
  "homepage": "https://github.com/crowdlift/frontendkit-tests",
  "scripts": {
    "build": "babel lib --out-dir dist/lib",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "nightwatch": "LAUNCH_URL='http://localhost:2000' nightwatch --config example/nightwatch.conf.js --verbose",
    "serve": "./node_modules/.bin/http-server -p 2000 example/dist",
    "test": "cross-env SINGLERUN=1 npm-run-all -p test:*",
    "test:browser": "cross-env NODE_ENV=test karma start example/karma.conf.js --log-level debug",
    "test:node": "cross-env NODE_ENV=test mocha --require babel-register example/tests.node.js",
    "test:e2e": "PORT=2000 ./bin/test_e2e_local.sh"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "expect": "^1.20.2",
    "frontendkit": "github:crowdlift/frontendkit",
    "karma": "^1.7.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "nightwatch": "^0.9.16",
    "selenium-standalone-bins": "github:crowdlift/selenium-standalone-bins#v1.1.0",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  },
  "peerDependencies": {},
  "devDependencies": {}
}
